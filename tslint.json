{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-console": false,
    "ordered-imports": [
      false
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "no-boolean-literal-compare": true,
    "ter-arrow-parens": false,
    "object-literal-sort-keys": false,
    "import-name": false,
    "variable-name":false,
    "max-line-length": [true, 150],
    "no-string-literal":false,
    "no-trailing-whitespace":false,
    "brace-style":false
    
  }
}
