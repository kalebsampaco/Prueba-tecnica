        "start": "env-cmd -f .env.dev craco start",
        "start:staging": "env-cmd -f .env.staging craco start",
        "start:prod": "env-cmd -f .env.produccion craco start",
        "start:preprod": "env-cmd -f .env.preproduction craco start",
        "build": "env-cmd -f .env.dev cross-env GENERATE_SOURCEMAP=false craco build",
        "build:staging": "env-cmd -f .env.staging cross-env GENERATE_SOURCEMAP=false craco build",
        "build:prod": "env-cmd -f .env.produccion cross-env GENERATE_SOURCEMAP=false craco build",
        "build:preprod": "env-cmd -f .env.preproduction cross-env GENERATE_SOURCEMAP=false craco build",
        "test": "craco test --env=node",
        "eject": "craco eject",
        "build-docs": "babel-node --presets @babel/preset-env src/app/main/documentation/material-ui-components/build.js",
        "analyze": "craco build && source-map-explorer 'build/static/js/*.js' --html analyze-result.html",
